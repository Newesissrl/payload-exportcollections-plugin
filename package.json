{
  "name": "payload-exportcollections-plugin",
  "version": "0.0.4",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "https://github.com/finkinfridom/payload-exportcollections-plugin.git",
  "author": "Alberto Maghini <alberto@newesis.com>",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "npx yarn-deduplicate yarn.lock",
    "version:show": "cat package.json | grep version",
    "dev": "tsc --watch",
    "build:tsc": "tsc --p tsconfig.components.json && tsc --p tsconfig.strategies.json",
    "build:components": "webpack --config webpack.components.config.js",
    "build": "rimraf dist/ && yarn build:tsc && yarn build:components",
    "test": "jest",
    "test:watch": "jest --watch",
    "serve:cov": "serve coverage -c ../serve.json",
    "lint": "eslint . --ext .ts",
    "push:cov": "yarn test && docker run -v $PWD:/code codacy/codacy-coverage-reporter report --coverage-reports /code/coverage/lcov.info"
  },
  "dependencies": {
    "@json2csv/plainjs": "^7.0.4",
    "babel-jest": "^29.7.0",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "file-loader": "^6.2.0",
    "payload": "^2.0.0",
    "react-jss": "^10.10.0"
  },
  "devDependencies": {
    "@types/jest": "29.5.11",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "babel-loader": "^9.1.3",
    "eslint": "^8.27.0",
    "jest": "29.7.0",
    "mini-css-extract-plugin": "^2.7.6",
    "serve": "^14.0.1",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.2",
    "typescript": "5.3.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  }
}
